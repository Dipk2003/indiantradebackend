# Vercel Frontend Environment Variables
# Add these to your Frontend Vercel project settings

# API Configuration
NEXT_PUBLIC_API_URL=https://your-backend-app.vercel.app
REACT_APP_API_URL=https://your-backend-app.vercel.app

# Authentication
NEXT_PUBLIC_JWT_SECRET=your-jwt-secret-key
REACT_APP_JWT_SECRET=your-jwt-secret-key

# Payment Gateway (Razorpay)
NEXT_PUBLIC_RAZORPAY_KEY_ID=your_razorpay_key_id
REACT_APP_RAZORPAY_KEY_ID=your_razorpay_key_id

# Google Services (if using)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id

# Analytics (if using)
NEXT_PUBLIC_GA_TRACKING_ID=your_google_analytics_id
REACT_APP_GA_TRACKING_ID=your_google_analytics_id

# Environment
NODE_ENV=production
NEXT_PUBLIC_NODE_ENV=production
REACT_APP_NODE_ENV=production

# Feature Flags
NEXT_PUBLIC_ENABLE_CHATBOT=true
REACT_APP_ENABLE_CHATBOT=true
NEXT_PUBLIC_ENABLE_PAYMENTS=true
REACT_APP_ENABLE_PAYMENTS=true

# External Services
NEXT_PUBLIC_MAPS_API_KEY=your_google_maps_api_key
REACT_APP_MAPS_API_KEY=your_google_maps_api_key

# CDN/Storage
NEXT_PUBLIC_CDN_URL=https://your-cdn-domain.com
REACT_APP_CDN_URL=https://your-cdn-domain.com

# Social Media
NEXT_PUBLIC_FACEBOOK_APP_ID=your_facebook_app_id
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id

# Support
NEXT_PUBLIC_SUPPORT_EMAIL=support@yourdomain.com
REACT_APP_SUPPORT_EMAIL=support@yourdomain.com
