option_settings:
  aws:elasticbeanstalk:application:environment:
    # Database Configuration (will be replaced with actual values)
    RDS_HOSTNAME: "your-rds-endpoint.ap-south-1.rds.amazonaws.com"
    RDS_PORT: "3306"
    RDS_DB_NAME: "itech_production"
    RDS_USERNAME: "admin"
    RDS_PASSWORD: "your-secure-password"
    
    # JWT Configuration
    JWT_SECRET: "your-super-secure-jwt-secret-key-should-be-256-bits-long"
    
    # AWS S3 Configuration
    S3_BUCKET_NAME: "itech-marketplace-files"
    AWS_REGION: "ap-south-1"
    AWS_ACCESS_KEY_ID: "your-access-key-id"
    AWS_SECRET_ACCESS_KEY: "your-secret-access-key"
    
    # Redis Configuration (ElastiCache)
    REDIS_HOST: "your-elasticache-endpoint.cache.amazonaws.com"
    REDIS_PORT: "6379"
    REDIS_PASSWORD: "your-redis-password"
    
    # Email Configuration (AWS SES)
    SES_USERNAME: "your-ses-smtp-username"
    SES_PASSWORD: "your-ses-smtp-password"
    
    # Payment Gateway Configuration
    RAZORPAY_KEY_ID: "your-razorpay-key-id"
    RAZORPAY_KEY_SECRET: "your-razorpay-key-secret"
    
    # Frontend URL for CORS
    FRONTEND_URL: "https://yourdomain.com"
    
    # Additional Environment Settings
    SPRING_PROFILES_ACTIVE: "aws-production"
    JAVA_OPTS: "-Xms512m -Xmx2048m -Dspring.profiles.active=aws-production"
    
    # Timezone Configuration
    TZ: "Asia/Kolkata"
