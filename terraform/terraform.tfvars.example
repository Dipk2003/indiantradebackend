# iTech B2B Marketplace - Terraform Variables
# Copy this file to terraform.tfvars and update with your actual values

# AWS Configuration
aws_region = "ap-south-1"  # Change to your preferred AWS region
environment = "production"

# Domain Configuration
domain_name = "yourdomain.com"  # Replace with your actual domain

# Database Configuration
db_username = "itech_admin"
db_password = "YourSecurePassword123!"  # Use a strong password

# Additional variables can be added here as needed
# Make sure to never commit the actual terraform.tfvars file with real credentials
